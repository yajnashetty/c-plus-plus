// Online C++ compiler to run C++ program online
#include <iostream>
#include<stack>
using namespace std;
int main() {
    stack<char> s;
    char val;
    int n;
    cout<<"enter the size of the stack";
    cin>>n;
    for(int i = 0; i<n; i++)
    {
    cout<<"enter the letter ";
    cin>>val;
    s.push(val);
    }
    
    while(!s.empty()){
        cout<<s.top()<<" ";
        s.pop();
    }
    cout << endl;
    
   

    return 0;
}
