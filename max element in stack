// Online C++ compiler to run C++ program online
#include <iostream>
#include<stack>
using namespace std;
int main() {
    stack<int> s;
    s.push(10);
    s.push(100);
    s.push(20);
    s.push(30);
    int max1 = 0;
   // cout<<s.top();
    
     while(!s.empty())
    {
   int n = s.top();
     max1 = max(n,max1);
     s.pop();
    }
    cout<<max1<<endl;
  
    return 0;
}
