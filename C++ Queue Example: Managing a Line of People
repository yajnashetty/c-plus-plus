
#include <iostream>
#include<queue>
using namespace std;
int main() {
queue <string> q;

   q.push("rudra");
    q.push("yajna");
     q.push("kruthi");
      q.push("keerthana");
       q.push("blesita");
        q.push("kusuma");
         q.push("santhosh");
          q.push("guddi");
           q.push("ridhan");
            q.push("rajesh");
   
 cout<<q.front()<<endl;
 cout<<q.back()<<endl;
 
for(int i = 0; i<2; i++)
{
    cout<<q.front()<<" ";
    q.pop();
}
cout<<endl;
int n = q.size();
cout<<"the remaining no of ppl in line "<<n;
    return 0;
}
